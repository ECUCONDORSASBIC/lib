    "crypto-browserify": "^3.12.1",
    "cypress": "^14.3.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "https-browserify": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^9.0.1",
    "imagemin-webp": "^8.0.0",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "leaflet": "^1.9.4",
    "node-loader": "^2.1.0",
    "pa11y-ci": "^3.0.1",
    "path-browserify": "^1.0.1",
