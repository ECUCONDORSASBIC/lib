    "process": "^0.11.10",
    "react-leaflet": "^5.0.0",
    "rimraf": "^5.0.5",
    "sharp": "^0.34.2",
    "start-server-and-test": "^2.0.12",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tailwindcss": "^3.4.4",
    "typescript": "5.8.3",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack": "^5.99.8",
    "webpack-cli": "^6.0.1"
  },
  "resolutions": {
    "crypto-browserify": "^3.12.1"
  }
}