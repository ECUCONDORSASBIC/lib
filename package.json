{
  "name": "pr-quality",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "node ./lib/build/ensure-directories.js",
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS=--inspect next dev",
    "build": "node --max_old_space_size=4096 lib/build/prebuild.js && next build --no-lint",
    "build:phased": "node --max_old_space_size=4096 lib/build/phased-build.js",
    "export": "next export",
    "build:export": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next",
    "cleanup": "node lib/build/cleanup.js",
    "prepare-dirs": "node lib/build/ensure-directories.js",
    "optimize-images": "node scripts/optimize-images.js",
    "test": "jest",
    "test:pnpm": "jest",
    "test:coverage": "jest --coverage",
    "test:a11y": "pa11y-ci './app/**/*.jsx'",
    "test:a11y:dashboard": "pa11y-ci './app/dashboard/**/*.jsx'",
    "test:a11y:auth": "pa11y-ci './app/auth/**/*.jsx'",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "firebase:emulate": "firebase emulators:start",
    "deploy": "next build && firebase deploy --only hosting",
    "deploy-static": "rimraf .next && rimraf out && next build && firebase deploy --only hosting",
    "test:e2e:auth": "start-server-and-test dev http://localhost:3000 cypress:auth",
    "test:e2e:patient": "start-server-and-test dev http://localhost:3000 cypress:patient",
    "test:e2e:doctor": "start-server-and-test dev http://localhost:3000 cypress:doctor",
    "test:e2e:admin": "start-server-and-test dev http://localhost:3000 cypress:admin",
    "test:e2e:telemedicine": "start-server-and-test dev http://localhost:3000 cypress:telemedicine",
    "test:e2e:a11y": "start-server-and-test dev http://localhost:3000 cypress:a11y",
    "test:e2e:documents": "start-server-and-test dev http://localhost:3000 cypress:documents",
    "test:e2e:all": "start-server-and-test dev http://localhost:3000 'cypress run'",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "jest",
    "test:watch": "jest --watch",
    "test:e2e:dashboard": "cypress run --spec 'cypress/e2e/doctor-dashboard-flow.cy.js'",
    "test:e2e:redirect": "cypress run --spec 'cypress/e2e/redirection-flow.cy.js'",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "@daily-co/daily-js": "^0.79.0",
